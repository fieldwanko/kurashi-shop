<header class="border-bottom">
  <div class="row pt-lg-4">
    <div class="headerTitle col-lg-3 text-center">
      <h1 class="text-dark">Kurashi Shop</h1>
    </div>
    <div class="headerSuport col-lg-1 text-center font-minimum">
      <%= link_to admins_users_path,class: "headerLine link-style font-minimum" do %>
        <span class="headerText"><%= fa_icon("users 2x") %><br>ユーザ一覧</span>
      <% end %>
    </div>
    <div class="headerSuport col-lg-1 text-center font-minimum">
      <%= link_to admins_sell_users_path,class: "headerLine font-minimum link-style" do %>
        <span class="headerText"><%= fa_icon("home 2x") %><br>販売店一覧</span>
      <% end %>
    </div>
    <div class="headerSuport col-lg-1 text-center font-minimum">
      <%= link_to admins_products_path,class: "headerLine font-minimum link-style" do %>
        <span class="headerText"><%= fa_icon("clipboard 2x") %><br>商品一覧</span>
      <% end %>
    </div>
    <div class="headerSuport col-lg-1 text-center font-minimum">
      <%= link_to new_admins_coupon_path,class: "headerLine font-minimum link-style" do %>
        <span class="headerText"><%= fa_icon("gift 2x") %><br>クーポン発行</span>
      <% end %>
    </div>
    <div class="headerSuport col-lg-1 text-center font-minimum">
      <%= link_to destroy_admin_session_path,class: "headerLine link-style font-minimum",method: :delete do %>
        <span class="headerText"><%= fa_icon("sign-out 2x") %><br>ログアウト</span>
      <% end %>
    </div>
  </div>
</header>